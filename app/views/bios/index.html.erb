<p id="notice"><%= notice %></p>

<h1>Bios</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Address</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Country</th>
      <th>Gender</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bios.each do |bio| %>
      <tr>
        <td><%= bio.user_id %></td>
        <td><%= bio.first_name %></td>
        <td><%= bio.last_name %></td>
        <td><%= bio.email %></td>
        <td><%= bio.phone_number %></td>
        <td><%= bio.address %></td>
        <td><%= bio.state %></td>
        <td><%= bio.zipcode %></td>
        <td><%= bio.country %></td>
        <td><%= bio.gender %></td>
        <td><%= bio.age %></td>
        <td><%= link_to 'Show', bio %></td>
        <td><%= link_to 'Edit', edit_bio_path(bio) %></td>
        <td><%= link_to 'Destroy', bio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bio', new_bio_path %>
